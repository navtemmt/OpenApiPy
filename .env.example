# cTrader API Credentials Template
# Copy this to .env and fill in your actual values

# Account naming convention: ACCOUNT_<NAME>_<FIELD>
# The <NAME> must match the account section in accounts_config.ini
# Example: [Account_Demo] requires ACCOUNT_DEMO_* variables

# Account 1: Demo
ACCOUNT_DEMO_ACCOUNT_ID=your_account_id_here
ACCOUNT_DEMO_CLIENT_ID=your_client_id_here
ACCOUNT_DEMO_CLIENT_SECRET=your_client_secret_here
ACCOUNT_DEMO_ACCESS_TOKEN=

# Account 2: Broker2
ACCOUNT_BROKER2_ACCOUNT_ID=your_account_id_here
ACCOUNT_BROKER2_CLIENT_ID=your_client_id_here
ACCOUNT_BROKER2_CLIENT_SECRET=your_client_secret_here
ACCOUNT_BROKER2_ACCESS_TOKEN=
